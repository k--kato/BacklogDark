@-moz-document regexp('^https?://.+\.backlog\..+/.*$') {

  /*-------------------------------------------------------------
   * The Dark Side of Backlog v1.0.0 (12/3/2016)
   * Author: Keisuke KATO
   * GitHub: https://github.com/k--kato/BacklogDark
   * Twitter: https://twitter.com/k_kato
   * License: https://creativecommons.org/licenses/by-sa/4.0/
   *-----------------------------------------------------------*/

  /*-------------------------------------------------------------
   * Common
   *-----------------------------------------------------------*/
  body {
    background-clip: border-box !important;
    background-origin: padding-box !important;
    background-size: auto !important;
    background: #222222 url('data:image/png;base64,') repeat scroll left top !important;
    font-family: Roboto, Droid Sans, '游ゴシック', YuGothic, 'ヒラギノ角ゴ ProN W3', Hiragino Kaku Gothic ProN, 'メイリオ', Meiryo, sans-serif, Arial !important;
    color: #CCCCCC !important;
  }

  a {
    color: #A9B3C9 !important;
  }

  a:hover {
    color: #00836b !important;
  }

  a:visited {
    color: #A9B3C9 !important;
  }

  /*-------------------------------------------------------------
   * login
   *-----------------------------------------------------------*/
  .Page--login-default .login-form__content {
    background-color: #222222 !important;
  }
  
  /*-------------------------------------------------------------
   * dashboard
   *-----------------------------------------------------------*/
  .global-header {
    background-color: #222222 !important;
    box-shadow: 0 1px 0 0 rgba(0, 0, 0, .23) !important;
  }

  .global-nav__link {
    color: #CCCCCC !important;
  }

  .search-box__input {
    border-color: #222222 !important;
    background-color: #222222 !important;
  }

  .dropdown-box {
    background-color: #222222 !important;
  }

  .data-list.-btm-border .data-list__item, .data-list.-top-btm-border .data-list__item {
    border-bottom: #222222 !important;
  }
  
  .switch-ui-button__inner {
    color: #CCCCCC !important;
  }

  .header-icon-set__name {
    color: #CCCCCC !important;
  }

  .dashboard-space-infomation {
    background-color: #222222 !important;
    border-color: #222222 !important;
  }

  .dropdown-menu {
    border-color: #222222 !important;
    background-color: #222222 !important;
  }

  .dropdown-menu__link:hover {
    background-color: #333333 !important;
  }

  .slide-in.-notifications, .slide-in.-watch-list {
    background-color: #222222 !important;
  }

  .search-box__input.is_disabled, .search-box__input[disabled] {
    color: #CCCCCC !important;
    background-color: #555555 !important;
  }

  .search-box.is_opened .search-box__input {
