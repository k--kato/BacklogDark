    border-color: #222222 !important;
    background-color: #555555 !important;
  }

  .search-box__input:focus {
    color: #CCCCCC !important;
    background-color: #555555 !important;
  }

  /*-------------------------------------------------------------
   * project
   *-----------------------------------------------------------*/
  .title {
    color: #CCCCCC !important;
  }

  .project-list__link {
    background-color: #222222 !important;
  }

  .project-list__item {
    border-color: #222222 !important;
  }

  .project-list__name {
    color: #CCCCCC !important;
  }

  .project-launch-menu__text {
    color: #CCCCCC !important;
  }

  .sort a:hover, .sort:hover {
    background-color: #333333 !important;
  }
  
  .core-wrapper.-settings .content-footer, .core-wrapper.-settings .content-wrapper {
    border-color: #222222 !important;
    background-color: #222222 !important;
  }
  
  /*-------------------------------------------------------------
   * data-controller
   *-----------------------------------------------------------*/
  .data-controller-box {
    border-color: #222222 !important;
    background-color: #222222 !important;
  }

  .data-table--default {
    border-color: #222222 !important;
    background-color: #222222 !important;
  }

  .modal--default {
    background-color: #222222 !important;
  }

  input.input-text {
    color: #CCCCCC !important;
    background-color: #555555 !important;
  }

  .timeline .button--weak {
    background-color: #555555 !important;
  }

  /*-------------------------------------------------------------
   * timeline
   *-----------------------------------------------------------*/
  .timeline__date {
    border-color: #222222 !important;
    background-color: #222222 !important;
  }

  .timeline__stream {
    border-color: #222222 !important;
    background-color: #222222 !important;
  }

  .timeline__stream-item.is_new, .timeline__stream-item.is_new:hover {
    background-color: #333333 !important;
  }

  .timeline__stream-item.is_deleted {
    background-color: #333333 !important;
  }
  
  .timeline__fixed-header {
    background-color: #222222 !important;
  }
  
  .comment-editor__preview-wrapper {
    border-color: #222222 !important;
    background-color: #555555 !important;
  }

  .timeline .comment-editor__action-wrapper {
    background-color: #555555 !important;
  }

  .comment-editor__emoticon-panel {
    border-color: #222222 !important;
    background-color: #222222 !important;
  }

  .add-comment-area__name {
    color: #CCCCCC !important;
  }
  
  /*-------------------------------------------------------------
   * project home
   *-----------------------------------------------------------*/
  .core-wrapper.-profile .content-header, .core-wrapper.-project .content-header, .core-wrapper.-settings .content-header {
    background-color: #222222 !important;
  }

  .project-user-modal__edit-permisstion {
    background-color: #222222 !important;
  }

  .card--default {
    border-color: #222222 !important;
    background-color: #222222 !important;
  }

  .project-nav-list__link.is_active, .project-nav-list__link:hover {
    background-color: #444444!important;
  }


  /*-------------------------------------------------------------
   * project issues
   *-----------------------------------------------------------*/
  .message--warning {
    background-color: #222222 !important;
  }

  .button-group {
    background-color: #222222 !important;
  }
  
  .selectbox__item {
    background-color: #222222 !important;
  }

  .chosen-wrapper .chzn-container-multi .chzn-choices, .chosen-wrapper .chzn-container-single .chzn-single {
    color: #CCCCCC !important;
    border-color: #222222 !important;
    background-color: #222222 !important;
  }

  .chosen-wrapper .chzn-container .chzn-drop {
    color: #CCCCCC !important;
    border-color: #222222 !important;
    background-color: #222222 !important;
  }

  .chosen-wrapper .chzn-container .chzn-results li.active-result {
    color: #CCCCCC !important;
  }

  .chosen-wrapper .chzn-container .chzn-results li.active-result.highlighted, .chosen-wrapper .chzn-container .chzn-results li.active-result.is_selected, .chosen-wrapper .chzn-container .chzn-results li.active-result.result-selected, .chosen-wrapper .chzn-container .chzn-results li.active-result:hover, .selectbox--multiple__item.is_selected, .selectbox--multiple__item.is_selected:hover, .selectbox--multiple__item:hover {
    background-color: #333333!important;
  }

  .chosen-wrapper .chzn-container .chzn-results .no-results {
    background: #333333 !important;
  }

  .Page--settings-project .tab--default .tab--default__link.is_active, .Page--settings-space .tab--default .tab--default__link.is_active, .Page--settings-user .tab--default .tab--default__link.is_active, .modal__content .tab--default .tab--default__link.is_active, .tab--default.-pull-request-fixed .tab--default__link.is_active, .tab--default.-with-tab-content-wrapper .tab--default__link.is_active {
    border-bottom-color: #333333 !important;
  }
  
  .Page--settings-project .tab--default .tab--default__link.is_active, .Page--settings-project .tab--default .tab--default__link:hover, .Page--settings-space .tab--default .tab--default__link.is_active, .Page--settings-space .tab--default .tab--default__link:hover, .Page--settings-user .tab--default .tab--default__link.is_active, .Page--settings-user .tab--default .tab--default__link:hover, .modal__content .tab--default .tab--default__link.is_active, .modal__content .tab--default .tab--default__link:hover, .tab--default.-pull-request-fixed .tab--default__link.is_active, .tab--default.-pull-request-fixed .tab--default__link:hover, .tab--default.-with-tab-content-wrapper .tab--default__link.is_active, .tab--default.-with-tab-content-wrapper .tab--default__link:hover {
    background-color: #222222 !important;
  }

  .Page--settings-project .tab--default .tab--default__link, .Page--settings-space .tab--default .tab--default__link, .Page--settings-user .tab--default .tab--default__link, .modal__content .tab--default .tab--default__link, .tab--default.-pull-request-fixed .tab--default__link, .tab--default.-with-tab-content-wrapper .tab--default__link {
    border-color: #333333 !important;
    background-color: #555555 !important;
  }

  .tab-content-wrapper {
    border-color: #333333 !important;
    background-color: #222222 !important;
  }

  .selectbox--multiple__wrapper {
    border-color: #333333 !important;
    background-color: #222222 !important;
  }

  .input-checkbox + label::before {
    border-color: #444444 !important;
    background-color: #333333 !important;
  }

  .modal--flex__wrapper {
    background-color: #222222 !important;
  }
  
  .Page--multipleIssue .button--weak, .Page--settings-project .button--weak, .Page--settings-space .button--weak, .Page--settings-user .button--weak, .button--weak, .button--weak.-on-card-base, .card--default .button--weak, .comment-editor.-fixed-footer .button--weak, .comment-item .button--weak, .global-header .button--weak, .modal__content .button--weak, .timeline .button--weak {
    background-color: #333333 !important;
  }

  .button--default {
    background-color: #333333 !important;
  }


  .Page--settings-project .comment-editor__action-wrapper, .Page--settings-space .comment-editor__action-wrapper, .Page--settings-user .comment-editor__action-wrapper, .card--default .comment-editor__action-wrapper, .comment-editor.-fixed-footer .comment-editor__action-wrapper, .comment-item .comment-editor__action-wrapper, .modal__content .comment-editor__action-wrapper, .timeline .comment-editor__action-wrapper {
    background-color: #333333 !important;
  }

  .Page--multipleIssue .icon-button--default, .Page--settings-project .icon-button--default, .Page--settings-space .icon-button--default, .Page--settings-user .icon-button--default, .card--default .icon-button--default, .card--default .loom .markdown-body code.loom_inner_code, .comment-editor .icon-button--default, .comment-item .icon-button--default, .content-header__inner .icon-button--default, .global-header .icon-button--default, .icon-button--default.-on-card-base, .modal__content .icon-button--default, .timeline .icon-button--default {
